  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1" />

    <title>FLAIR-GG Virtual Platform</title>

    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <link rel="stylesheet" href="/stylesheets/main.css" />
  
  </head>
  <body>
    <div class="wrapper">
    <h2>Discovered VP Resources</h2>
    <ol>

    <% @discoverables.each_key do |url| %>
      <% type = @discoverables[url][:type] %>
      <% type = type.match(/[\#\/](\w+?)$/)[1].downcase %>
      <li><img src="../images/<%= type %>.svg" width=35/> &nbsp;&nbsp;Resource:
      <a href="<%= url %>" > <%= @discoverables[url][:title] %> </a>
      Type <%= type.capitalize %></li>
    <% end %>
      </ol>
      <footer class="branding">
        <small>&copy; 2023 <strong>Mark D Wilkinson</strong></small>
      </footer>
    </div>

    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
    <!--[if lt IE 7]>
      <script src="//ajax.googleapis.com/ajax/libs/chrome-frame/1.0.2/CFInstall.min.js"></script>
      <script>window.attachEvent("onload",function(){CFInstall.check({mode:"overlay"})})</script>
    <![endif]-->
  </body>
</html>
